{
  "domains": [
    {
      "name": "doubleclick.app",
      "redirects": [
        {
          "source": "/(.*)",
          "destination": "https://doubleclick.app/$1",
          "permanent": false
        }
      ]
    },
    {
      "name": "www.doubleclick.app",
      "redirects": [
        {
          "source": "/(.*)",
          "destination": "https://doubleclick.app/$1",
          "permanent": true
        }
      ]
    }
  ],
  "ssl": {
    "force": true,
    "redirect": true
  },
  "cdn": {
    "enabled": true,
    "cache": {
      "static": "1y",
      "api": "1h"
    }
  }
}
